<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Outer Circle -->
    <item>
        <shape
            android:shape="oval">
            <solid android:color="#FFFFFF" /> <!-- White color -->
        </shape>
    </item>
    <!-- Inner Circle (Hollow) -->
    <item
        android:right="6dp"
        android:left="6dp"
        android:top="6dp"
        android:bottom="6dp">
        <shape
            android:shape="oval">
            <solid android:color="@android:color/transparent" /> <!-- Transparent color -->
            <stroke
                android:width="3dp"
                android:color="#FFFFFF" /> <!-- White color -->
        </shape>
    </item>
</layer-list>